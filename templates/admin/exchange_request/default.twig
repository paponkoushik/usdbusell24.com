{% extends 'layouts/application.twig' %}
{% block title %} Money Provider {% endblock %}
{% block breadcumbs %}
    <div class="breadcrumbs">
        <div class="breadcrumbs-inner">
            <div class="row m-0">
                <div class="col-sm-4">
                    <div class="page-header float-left">
                        <div class="page-title">
                            <h1>Exchange Request</h1>
                        </div>
                    </div>
                </div>
                <div class="col-sm-8">
                    <div class="page-header float-right">
                        <div class="page-title">
                            <ol class="breadcrumb text-right">
                                <li><a href="#">Exchange Request</a></li>
                                <li><a href="#">All</a></li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}

{% block content %}
    <div class="row">
        <div class="col-lg-12">
            <div class="card">
                <div class="card-header">
                    <strong class="card-title">Exchange Request</strong>
                </div>
                <div class="card-body">
                    <table class="table">
                        <thead>
                        <tr>
                            <th scope="col">#</th>
                            <th scope="col">Name</th>
                            <th scope="col">Amount</th>
                            <th scope="col">Action</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <th scope="row">1</th>
                            <td>Rocket Agent</td>
                            <td>120</td>
                            <td>
                                <a href="#" class="btn btn-success btn-sm">Accept</a>
                                <a href="#" class="btn btn-danger btn-sm">Decline</a>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">2</th>
                            <td>Bikash Agent</td>
                            <td>123</td>
                            <td>
                                <a href="#" class="btn btn-success btn-sm">Accept</a>
                                <a href="#" class="btn btn-danger btn-sm">Decline</a>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">3</th>
                            <td>Rocket Personal</td>
                            <td>455</td>
                            <td>
                                <a href="#" class="btn btn-success btn-sm">Accept</a>
                                <a href="#" class="btn btn-danger btn-sm">Decline</a>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">4</th>
                            <td>Skrill Personal</td>
                            <td>455</td>
                            <td>
                                <a href="#" class="btn btn-success btn-sm">Accept</a>
                                <a href="#" class="btn btn-danger btn-sm">Decline</a>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">5</th>
                            <td>Coinbase</td>
                            <td>455</td>
                            <td>
                                <a href="#" class="btn btn-success btn-sm">Accept</a>
                                <a href="#" class="btn btn-danger btn-sm">Decline</a>
                            </td>
                        </tr>
                        <tr>
                            <th scope="row">6</th>
                            <td>Dollar Exchange</td>
                            <td>455</td>
                            <td>
                                <a href="#" class="btn btn-success btn-sm">Accept</a>
                                <a href="#" class="btn btn-danger btn-sm">Decline</a>
                            </td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
{% endblock %}